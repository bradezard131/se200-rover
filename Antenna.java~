/** Antenna.java
 * 
 * Author: Bradley Ezard
 * Info: Controller for Mars Rover communication antenna.
 */

import java.util.*;
import java.io.*;

public class Antenna extends Comm {
    //-- Constructor --//
    public Antenna() {}

    //-- Instance Methods --//
    @Override
    public void receive(String message) {
	deserialize(message);

	//Store
    }

    @Override
    public void send(Serializable s) {
	this.send(serialize(s));
    }

    //(De)Serialization
    
